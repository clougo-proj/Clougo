{
    "name": "clougo",
    "version": "0.0.1",
    "description": "clougo",
    "devDependencies": {
        "eslint": "latest",
        "http-server": "latest"
    },
    "scripts": {
        "clean": "rm generated/*.js",
        "eslint": "eslint logo",
        "pkmod": "node tools/pkdir mod > generated/mod.js",
        "pkut": "node tools/pkut unittests > generated/unittests.js",
        "pkdemo": "node tools/pkdir demo > generated/demo.js",
        "pkucblogohelp": "node tools/pkdir UCBLogo > generated/UCBLogo.js",
        "build": "npm run eslint && npm run pkdemo && npm run pkut && npm run pkmod && npm run pkucblogohelp",
        "test": "node logo/logo --test --on:verbose && node logo/logo --test --on:postfix,verbose",
        "server": "http-server",
        "predeploy": "rm -rf unittests && rm -rf demo && rm -rf node_modules && rm -rf UCBLogo"
    }
}
